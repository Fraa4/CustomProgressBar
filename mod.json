{
	"geode": "5.0.1",
	"gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
	},
	"id": "fra4.custom-progress-bar",
	"name": "Custom ProgressBar",
	"version": "v1.0.9",
	"developer": "Fra4",
	"description": "A mod that lets you customize the progress bar",
	"tags": ["offline","utility","customization"],
	"links": {"source": "https://github.com/Fraa4/CustomProgressBar"},
	"settings": {
		"vertical-progress-bar": {
			"type": "bool",
			"name": "Vertical progress bar",
			"default": false
		},
		"Height": {
			"type": "string",
			"name": "Height",
			"default": "Top",
			"one-of": ["Top", "Bottom"]
		},
		"Position": {
			"type": "string",
			"name": "Position",
			"default": "Center",
			"one-of": ["Left", "Center", "Right"]
		},
		"Scale": {
			"type": "float",
			"name": "Scale",
			"default": 1,
			"min": 0.1,
			"max": 2,
			"controls": {
				"arrows": true,
				"arrow-step": 0.1,
				"big-arrows": false,
				"big-arrow-step": 0.5,
				"slider": true,
				"slider-step": 0.1,
				"input": false
			}
		},
		"x-offset": {
			"type": "int",
			"name": "x offset",
			"default": 0,
			"min": -100,
			"max": 100,
			"controls": {
				"arrows": true,
				"arrow-step": 1,
				"big-arrows": false,
				"big-arrow-step": 1,
				"slider": true,
				"slider-step": 1,
				"input": false
			}
		},
		"y-offset": {
			"type": "int",
			"name": "y offset",
			"default": 0,
			"min": -100,
			"max": 100,
			"controls": {
				"arrows": true,
				"arrow-step": 1,
				"big-arrows": false,
				"big-arrow-step": 1,
				"slider": true,
				"slider-step": 1,
				"input": false
			}
		},
		"Opacity":{
			"type": "float",
			"name": "Opacity",
			"default": 1,
			"min": 0,
			"max": 1,
			"controls": {
				"arrows": true,
				"arrow-step": 0.1,
				"big-arrows": false,
				"big-arrow-step": 0.5,
				"slider": true,
				"slider-step": 0.1,
				"input": false
			}
		}
	}
}
